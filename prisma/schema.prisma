// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")

}

generator client {
  provider = "prisma-client-js"
}

model Peoples {
  id Int @default(autoincrement()) @id
  name String
}

model Operations {
  id Int @default(autoincrement()) @id
  name String
}

model Status {
  id Int @default(autoincrement()) @id
  name String
}

model Services {
  id Int @default(autoincrement()) @id
  name String
}

model debit {
  id Int @default(autoincrement()) @id
  service String
  account String
  bank String
  description String?
  document String
  expiry DateTime
  operation String
  agency String?
  paymentBy String
  paymentDate DateTime
  pix String
  provider String
  requestedBy String
  status String
  value String
  created_at DateTime @default(now()) @map(name: "created_at")
  updated_at DateTime @default(now()) @map(name: "updated_at")
}